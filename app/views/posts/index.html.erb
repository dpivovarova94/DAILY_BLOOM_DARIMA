<div class="container_feed" data-controller="posts-toggle">
<%# <div class="posts-iteration" data-controller="posts-toggle"> %>
  <p data-action="click->posts-toggle#selectMyPosts">Me</p>
  <p data-action="click->posts-toggle#selectFriendsPosts">My Friends</p>
  <div class="button-selection">
    <%= link_to 'Quote', feed_path(medium: (params[:medium] == 'text' ? nil : 'text')), class: 'btn-feed btn-quote' %>
    <%= link_to 'Song', feed_path(medium: (params[:medium] == 'song_url' ? nil : 'song_url')), class: 'btn-feed btn-song' %>
    <%= link_to 'Poem', feed_path(medium: (params[:medium] == 'poem' ? nil : 'poem')), class: 'btn-feed btn-poem' %>
    <%= link_to 'Photo', feed_path(medium: (params[:medium] == 'photo' ? nil : 'photo')), class: 'btn-feed btn-photo' %>
  </div>

  <div data-posts-toggle-target="myPosts" >
    <div class="feed">
      <% @my_posts.each do |post| %>
        <%= render partial: 'posts', locals: { post: post } %>
      <% end %>
    </div>
  </div>

  <div data-posts-toggle-target="friendsPosts" class="d-none" >
    <div class="feed">
      <% @friends_posts.each do |post| %>
        <%= render partial: 'posts', locals: { post: post } %>
      <% end %>
    </div>
  </div>

  </div>
<%# </div> %>
